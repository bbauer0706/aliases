{
  "general": {
    "editor": "code",
    "terminal_colors": true,
    "verbosity": "normal",
    "confirm_destructive_actions": true
  },
  "code": {
    "vscode_flags": [],
    "reuse_window": true,
    "fallback_behavior": "auto",
    "preferred_component": "server"
  },
  "todo": {
    "default_priority": 0,
    "default_sort": "priority",
    "show_completed": false,
    "auto_categorize": false
  },
  "env": {
    "base_port": 3000,
    "port_offset": 100,
    "default_env": "dev"
  },
  "sync": {
    "enabled": false,
    "auto_sync": {
      "enabled": false,
      "interval": 86400
    },
    "last_sync": 0,
    "config_file_url": "",
    "todo_file_url": ""
  },
  "projects": {
    "workspace_directories": [
      "~/workspaces"
    ],
    "shortcuts": {},
    "server_paths": {},
    "web_paths": {},
    "ignore": [],
    "default_paths": {
      "server": [
        "java/serverJava",
        "serverJava",
        "backend",
        "server"
      ],
      "web": [
        "webapp",
        "webApp",
        "web",
        "frontend",
        "client"
      ]
    }
  }
}
